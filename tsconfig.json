{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/components/ui/fromFields.jsx", "src/components/addVehicle", "src/components/auth/loginUsingOtp.jsx", "src/app/(home)/(superAdmin)/userManagement/[profileId]", "src/components/DataFetch/DataFetch.js", "src/components/tables/dataTable.jsx", "src/components/clientLevelUser/vehicles/instockVehicles/initiateInstockVehicle.tsx", "src/components/modals/confirmOwnership/confirmOwnership.tsx", "src/components/clientLevelUser/vehicleOwnership"],
  "exclude": ["node_modules"]
}
